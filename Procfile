release: python -c "import os; import sys; import logging; logging.basicConfig(level=logging.INFO); api_key = os.getenv('TMDB_API_KEY'); logging.info('Starting data generation...'); logging.info(f'TMDB API Key present: {bool(api_key)}'); from generate_data import *; __main__(); logging.info('Data generation completed')"
web: gunicorn app:app --bind 0.0.0.0:$PORT --timeout 120 